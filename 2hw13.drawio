<?xml version="1.0" encoding="UTF-8"?>
<mxfile host="app.diagrams.net">
  <diagram name="Ticket Vending State Diagram" id="TicketVendingStateDiagram">
    <mxGraphModel dx="1200" dy="800" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="827" pageHeight="1169" math="0">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- States -->
        <mxCell id="idle" value="Idle" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#cfe2f3;strokeColor=#6c8ebf;" vertex="1" parent="1">
          <mxGeometry x="300" y="40" width="120" height="60" as="geometry"/>
        </mxCell>
        <mxCell id="waitingMoney" value="WaitingForMoney" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f9cb9c;strokeColor=#e69138;" vertex="1" parent="1">
          <mxGeometry x="300" y="140" width="160" height="60" as="geometry"/>
        </mxCell>
        <mxCell id="moneyReceived" value="MoneyReceived" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d9ead3;strokeColor=#6aa84f;" vertex="1" parent="1">
          <mxGeometry x="300" y="260" width="140" height="60" as="geometry"/>
        </mxCell>
        <mxCell id="ticketDispensed" value="TicketDispensed" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe599;strokeColor=#bf9000;" vertex="1" parent="1">
          <mxGeometry x="300" y="380" width="140" height="60" as="geometry"/>
        </mxCell>
        <mxCell id="transactionCanceled" value="TransactionCanceled" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f4cccc;strokeColor=#cc0000;" vertex="1" parent="1">
          <mxGeometry x="500" y="200" width="160" height="60" as="geometry"/>
        </mxCell>

        <!-- Transitions -->
        <mxCell id="t1" style="edgeStyle=orthogonalEdgeStyle;rounded=0;endArrow=blockThin;" edge="1" parent="1" source="idle" target="waitingMoney">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t2" style="edgeStyle=orthogonalEdgeStyle;rounded=0;endArrow=blockThin;" edge="1" parent="1" source="waitingMoney" target="moneyReceived">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="t3" style="edgeStyle=orthogonalEdgeStyle;rounded=0;endArrow=blockThin;" edge="1" parent="1" source="moneyReceived" target="ticketDispensed">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- Cancel Transitions -->
        <mxCell id="c1" style="edgeStyle=orthogonalEdgeStyle;rounded=0;dashed=1;endArrow=blockThin;" edge="1" parent="1" source="waitingMoney" target="transactionCanceled">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="c2" style="edgeStyle=orthogonalEdgeStyle;rounded=0;dashed=1;endArrow=blockThin;" edge="1" parent="1" source="moneyReceived" target="transactionCanceled">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- Optional: Return to Idle after cancel -->
        <mxCell id="r1" style="edgeStyle=orthogonalEdgeStyle;rounded=0;endArrow=blockThin;" edge="1" parent="1" source="transactionCanceled" target="idle">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- Optional: Return to Idle after successful dispense -->
        <mxCell id="r2" style="edgeStyle=orthogonalEdgeStyle;rounded=0;endArrow=blockThin;" edge="1" parent="1" source="ticketDispensed" target="idle">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
